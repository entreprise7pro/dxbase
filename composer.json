{
    "name": "entreprise7pro/dxbase",
    "description": "WxT optimal base for Drupal 8/9 sites built with the WxT distribution.",
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "behat/behat": "^3.0",
        "behat/mink": "~1.7",
        "behat/mink-extension": "*",
        "behat/mink-goutte-driver": "*",
        "behat/mink-selenium2-driver": "*",
        "drupal/coder": "^8.3.7",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "pdepend/pdepend": "2.12.1",
        "phpmd/phpmd": "@stable",
        "mikey179/vfsstream": "^1.6",
        "sebastian/phpcpd": "*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "config": {
        "secure-http": false,
        "prestissimo": {
            "maxConnections": 10,
            "minConnections": 5
        },
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/console-extend-plugin": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "composer",
            "url": "https://drupalwxt.github.io/composer-extdeps/"
        }
    ],
    "extra": {
        "merge-plugin": {
            "include": [
                "html/modules/contrib/webform/composer.libraries.json"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "html/"
            }
        },
        "installer-types": [
          "bower-asset",
          "npm-asset",
          "custom-module",
          "custom-theme"
        ],
        "installer-paths": {
            "html/core": [
                "type:drupal-core"
            ],
            "html/libraries/{$name}" : [
                "type:drupal-console-library",
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "html/modules/custom/{$name}": [
                "drupal/wxt_library",
                "type:drupal-custom-module"
            ],
            "html/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "html/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "html/themes/custom/{$name}": [
                "drupal/wxt_bootstrap",
                "type:drupal-custom-theme"
            ],
            "html/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
               "BigPipe when server under heavy load results in PHP notices":
                "https://www.drupal.org/files/issues/2021-11-19/D93x-3194462-20.patch",
